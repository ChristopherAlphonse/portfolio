  "version": "0.0.1",
  "description": "Christopher Alphonse portfolio V1",
  "homepage": "https://christopheralphonse.com/",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "start:dev": "concurrently \"vite\" \"node --experimental-modules server/index.js\"",
    "build": "vite build --mode production",
    "preview": "vite build && yarn vite preview",
    "deploy": " vite build && firebase deploy --only hosting",
    "firebase": "firebase login && firebase init",
    "lint": "eslint src --ext .js,.jsx",
    "lint:fix": "eslint src --ext .js,.jsx --fix",
    "checked": "ncu && ncu -u && yarn install && yarn autoclean --force",
    "prepare": "husky install",
    "eol": "eol -crlf server/index.js --force"
  },
